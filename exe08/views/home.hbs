<h1 class="title">Magasin</h1>
<div class="stores">
    {{#each magasins as |magasin|}}
    <div class="store store__hero">
        <a href="/magasins/{{magasin.slug}}">
        <img src="/uploads/{{magasin.photo}}" alt="{{magasin.description}}" class="single__image">
            <h2 class="title">{{magasin.name}}</h2>
            
        </a>
    </div>
    {{/each}}
</div>